<div>
  <main>
    <div class="row">
      <div >
        {{#with navigation}}
          {{#each links}}
            <a class="btn btn-link" href={{href}}>{{text}} </a>
          {{/each}}
          <a class="btn btn-primary" href={{button.href}}>{{button.text}} </a>
        {{/with}}
      </div>
      <hr>

      {{#with header}}
        <h1> {{title}} </h1>
        <p> {{description}} </p>
        <p><a class="btn btn-primary col-2" href={{button.link}}>{{button.text}} </a></p>
      {{/with}}
      <hr>
      <section class="col-12">
        {{#with city}}
          <h2> {{title}} </h2>       
          <div class="row g-2">
            {{#each cards}}
              <div class="col-5">              
                <div class="card">                               
                  <img class="card-img-top" src={{picture.src}} alt={{picture.alt}}>
                  <div class="card-body">
                  <h6 class="card-title "> {{title}}</h6>
                  <p>{{description}}</p>
                  </div>
                  <p><a class="btn btn-primary " href={{button.href}}>{{button.text}} </a></p>
                </div>
              </div>                                         
            {{/each}}
          </div>           
        {{/with}}
      </section>

      <hr>
      <section>
        {{#with winner}}
          <h2> {{title}} </h2> 
          <br> 
          {{#with table}}
            <table class="table table-striped table-bordered ">
              <thead>
                <tr>                
                  {{#each head}}
                    <th>{{this}}</th>
                  {{/each}}              
                </tr>
              </thead>
              <tbody>            
                {{#each user}}
                  <tr>
                    <th> {{place}} </th>
                    <td> {{name}} </td>
                    <td> {{surname}} </td>
                    <td> {{nickname}} </td>
                  </tr>
                {{/each}}              
              </tbody>
            </table>
          {{/with}}
        {{/with}}
      </section>

      <hr>

      <section class="col-12">        
        {{#with gallery}}
          <h2> {{title}} </h2>
            <div class="row g-2">
              {{#each cards}}
                <div class="col-4">
                  <div class="card">                  
                    <div class="card-header">
                      <div>{{name}}</div>           
                    </div>
                    <ul class="list-group list-group-flush">
                      {{#each items}}
                        <li class="list-group-item">
                          <div>{{this}}</div>
                        </li>
                      {{/each}}             
                    </ul>                                
                  </div>
                </div>              
              {{/each}}
            </div>
        {{/with}}
      </section>
      <hr>

      <section class="col-12">
        {{#with relaxion}}
          <div class="display-3"> {{title}} </div>
          <br>
          <div class="row gx-2 gy-5">
            {{#each cards}}
              <div class="col-5">
                <div class="card text-bg-light">
                  <div class="card-header text-secondary"> {{header}} </div>
                  <div class="card-body ">
                    <h6 class="card-title">{{title}}</h6>
                    <p>{{description}}</p>
                  </div>
                </div>
              </div>
            {{/each}}            
          </div>          
        {{/with}}
      </section>
      <hr>

      <section class="col-12">
        {{#with rest}}
          <div class="row g-2">
            {{#each cards}}
              <div class="col-6">
                <div class="card">
                  <div class="card-header text-secondary text-center"> {{header}} </div>
                  <div class="card-body text-center">
                    <h6 class="card-title">{{title}}</h6>
                    <p>{{description}}</p>
                    <button class="btn btn-primary">{{button}}</button>
                  </div>
                  <div class="card-footer text-center">{{footer}}</div>
                </div>
              </div>
            {{/each}}
          </div>
        {{/with}}
      </section>

      <hr>
      <section class="col-12">
        {{#with proposition}}
          <div class="display-2"> {{title}} </div>
          <br>
          <div class="row g-2">
            {{#each card}}
              <div class="col-5">
                <div class="card text-bg-warning">
                  <div class="card-header text-secondary "> {{header}} </div>
                  <div class="card-body">
                    <h6 class="card-title">{{title}}</h6>
                    <p>{{description}}</p>                    
                  </div>                  
                </div>
              </div>
            {{/each}}
          </div>
        {{/with}}
      </section>
      <hr>


      <section class="col-12">
        {{#with footer}}
          <h5> {{title}} </h5>        
          <div class="row">
            <section class="col-3">
              {{#each navigation}}                
                <p><a href={{href}} class="btn btn-link btn-sm">{{text}}</a></p>                
              {{/each}}
            </section>   
            <section class="col-3">
            {{#each socialMedia}}              
              <p><a href={{link}} class="btn btn-outline-dark btn-sm">{{name}}</a></p>              
            {{/each}}
            </section>
            <section class="col-4">
            {{#each partners}}              
              <p><a href={{link}} class="btn btn-outline-info btn-sm">{{name}}</a></p>              
            {{/each}}
            </section>
          </div>          
        {{/with}}
      </section>
    </div>    
  </main>
</div>


